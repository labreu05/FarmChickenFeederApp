<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.labreu.farmchickenfeederapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NavegationDrawer"
            android:label="FarmChickenFeedApp"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateScheduleActivity"
            android:parentActivityName=".NavegationDrawer">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NavegationDrawer" />
        </activity>
    </application>

</manifest>